{
    "FREERTOS": [ " ../../.." ],
    "PROOFS": [ "." ],

    "DEF ": [
	"_DEBUG",
	"__free_rtos__",
	"_CONSOLE",
	"_WIN32_WINNT=0x0500",
	"WINVER=0x400",
	"_CRT_SECURE_NO_WARNINGS",
	"__PRETTY_FUNCTION__=__FUNCTION__"
    ],

    "INC ": [
	"$(FREERTOS)/demos/pc/windows/common/config_files",
	"$(FREERTOS)/demos/pc/windows/common/application_code/include",
	"$(FREERTOS)/demos/common/include",
	"$(FREERTOS)/lib/include",
	"$(FREERTOS)/lib/include/private",
	"$(FREERTOS)/lib/FreeRTOS/include",
	"$(FREERTOS)/lib/FreeRTOS/portable/MSVC-MingW",
	"$(FREERTOS)/lib/FreeRTOS-Plus-TCP/include",
	"$(FREERTOS)/lib/FreeRTOS-Plus-TCP/source/portable/BufferManagement",
	"$(FREERTOS)/lib/FreeRTOS-Plus-TCP/source/portable/Compiler/MSVC",
	"$(FREERTOS)/lib/ota/portable/pc/windows",
	"$(FREERTOS)/lib/third_party/mbedtls/include",
	"$(FREERTOS)/lib/third_party/tracealyzer_recorder/Include",
	"$(FREERTOS)/lib/third_party/jsmn",
	"$(FREERTOS)/lib/third_party/pkcs11",
	"$(FREERTOS)/lib/third_party/tinycbor",
	"$(FREERTOS)/lib/third_party/win_pcap",
	"$(FREERTOS)/lib/cbor/src",
	"$(FREERTOS)/lib/defender/src",
	"$(FREERTOS)/lib/defender/metrics",
	"$(FREERTOS)/lib/defender/report",
	"../../include"
    ],

    "CBMCFLAGS ": [
	"--object-bits 7",
	"--32",
	"--bounds-check",
	"--pointer-check"
    ]
}

